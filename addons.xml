<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.leinad4mind.xbmc" name="Leinad4Mind Repositório" version="1.0.0" provider-name="Leinad4Mind">
	<extension point="xbmc.addon.repository" name="Leinad4Mind Add-on Repository">
		<info compressed="false">http://raw.github.com/Leinad4Mind/Leinad4Mind-xbmc-addons/master/addons.xml</info>
		<checksum>http://raw.github.com/Leinad4Mind/Leinad4Mind-xbmc-addons/master/addons.xml.md5</checksum>
		<datadir zip="true">http://raw.github.com/Leinad4Mind/Leinad4Mind-xbmc-addons/master/repo</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Repositorio Leinad4Mind</summary>
		<description>Addons de Leinad4Mind, contacto: leinad4talk@gmail.com</description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>

<addon
  id="skin.confluence.custom.mod"
  version="5.0.1"
  name="Confluence Customizable Mod"
  provider-name="Mudislander / Angelinas1 / Leinad4Mind">
  <requires>
	<import addon="xbmc.gui" version="5.3.0"/>
	<import addon="script.favourites" version="5.0.2"/>
	<import addon="service.skin.widgets" version="0.0.27"/>
	<import addon="script.randomandlastitems" version="2.2.0"/>
  </requires>
  <extension
    point="xbmc.gui.skin"
    defaultthemename="textures.xbt"
    debugging="false"
    effectslowdown="0.75">
    <res width="1920" height="1080" aspect="16:9" default="true" folder="16x9" />
  </extension>
  <extension point="xbmc.addon.metadata">
	<platform>all</platform>
	<summary>Uma versão modificada do tema Confluence de Jezz_X</summary>
	<disclaimer>-</disclaimer>
	<description>Uma modificação do tema padrão do XBMC, incorporando muitos dos seus famosos ajustes, addons e scripts.</description>
  </extension>
</addon>
</addons>
